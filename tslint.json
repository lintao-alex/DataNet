{
  "extends": ["tslint:recommended", "tslint-config-prettier"],
  "rules": {
    "prefer-const": false,
    "no-var-keyword": true,
    "no-console": true,
    "object-literal-sort-keys": false,
    "member-access": false,
    "ordered-imports": false
  },
  "linterOptions": {
    "exclude": ["**/*.json", "node_modules", "test/**/*"]
  }
}
